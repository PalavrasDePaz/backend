    "inversify": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.2",
    "mysql2": "^2.3.3",
    "sequelize": "^6.25.2",
    "swagger-ui-express": "^4.6.2",
    "tsoa": "^5.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/bcrypt": "^5.0.0",
